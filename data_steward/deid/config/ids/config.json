        "response_id": [
            "(SELECT ",
            "CASE WHEN :value_field IS NULL THEN NULL ",
            "ELSE (",
                "SELECT dqrm.research_response_id ",
                "FROM :idataset._deid_questionnaire_response_map AS dqrm ",
                "WHERE dqrm.questionnaire_response_id = :value_field) ",
